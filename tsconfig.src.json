{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@styles/*": ["src/styles/*"],
      "@stores/*": ["src/stores/*"],
      "@assets/*": ["src/assets/*"],
      "@views/*": ["src/views/*"],
      "@configs/*": ["src/configs/*"],
      "@interfaces/*": ["src/interfaces/*"],
      "@data/*": ["src/data/*"],
      "@composables/*": ["src/composables/*"]
    }
  }
}
